data;\n\n          if (response.status) {\n            window.helper.showMessage('success', vm);\n            var auth_data = {\n              admin: response.data.admin,\n              token: response.data.token\n            };\n            window.ls.saveToStorage('auth_data', auth_data);\n            vm.$router.push({\n              name: 'home'\n            });\n            return null;\n          }\n        }).catch(function (error) {\n          window.helper.handleError(error, vm);\n        });\n      } catch (e) {\n        console.log(e);\n      }\n    }\n  }\n};",{"version":3,"sources":["Login.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AA0DA,eAAA;AACA,EAAA,IADA,kBACA;AACA,WAAA;AACA,MAAA,QAAA,EAAA,EADA;AAEA,MAAA,QAAA,EAAA,EAFA;AAGA,MAAA,oBAAA,EAAA;AAHA,KAAA;AAKA,GAPA;AAQA,EAAA,OAAA,EAAA;AACA,IAAA,KADA,mBACA;AACA;AACA,UAAA,EAAA,GAAA,IAAA;AACA,MAAA,CAAA,CAAA,uBAAA,CAAA,CAAA,IAAA,CAAA,EAAA;;AACA,UAAA;AACA,QAAA,MAAA,CAAA,UAAA,CAAA,GAAA,GAAA,IAAA,CAAA,MAAA,CAAA,UAAA,CAAA,UAAA,EAAA;AACA,UAAA,QAAA,EAAA,EAAA,CAAA,QADA;AAEA,UAAA,QAAA,EAAA,EAAA,CAAA;AAFA,SAAA,EAIA,IAJA,CAIA,UAAA,QAAA,EAAA;AACA,UAAA,QAAA,GAAA,QAAA,CAAA,IAAA;;AACA,cAAA,QAAA,CAAA,MAAA,EAAA;AACA,YAAA,MAAA,CAAA,MAAA,CAAA,WAAA,CAAA,SAAA,EAAA,EAAA;AACA,gBAAA,SAAA,GAAA;AACA,cAAA,KAAA,EAAA,QAAA,CAAA,IAAA,CAAA,KADA;AAEA,cAAA,KAAA,EAAA,QAAA,CAAA,IAAA,CAAA;AAFA,aAAA;AAIA,YAAA,MAAA,CAAA,EAAA,CAAA,aAAA,CAAA,WAAA,EAAA,SAAA;AACA,YAAA,EAAA,CAAA,OAAA,CAAA,IAAA,CAAA;AAAA,cAAA,IAAA,EAAA;AAAA,aAAA;AACA,mBAAA,IAAA;AACA;AACA,SAhBA,EAgBA,KAhBA,CAgBA,UAAA,KAAA,EAAA;AACA,UAAA,MAAA,CAAA,MAAA,CAAA,WAAA,CAAA,KAAA,EAAA,EAAA;AACA,SAlBA;AAmBA,OApBA,CAoBA,OAAA,CAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,CAAA;AACA;AACA;AA5BA;AARA,CAAA","sourcesContent":["<!-- =========================================================================================\n    File Name: Login.vue\n    Description: Login Page\n    ----------------------------------------------------------------------------------------\n    Item Name: Vuesax Admin - VueJS Dashboard Admin Template\n      Author: Pixinvent\n    Author URL: http://www.themeforest.net/user/pixinvent\n========================================================================================== -->\n\n\n<template>\n  <div class=\"h-screen flex w-full bg-img vx-row no-gutter items-center justify-center\" id=\"page-login\">\n    <div class=\"vx-col sm:w-1/2 md:w-1/2 lg:w-3/4 xl:w-3/5 sm:m-0 m-4\">\n      <vx-card>\n        <div slot=\"no-body\" class=\"full-page-bg-color\">\n          <div class=\"vx-row no-gutter justify-center items-center\">\n            <div class=\"vx-col hidden lg:block lg:w-1/2\">\n              <img src=\"@/assets/images/pages/login.png\" alt=\"login\" class=\"mx-auto\">\n            </div>\n            <div class=\"vx-col sm:w-full md:w-full lg:w-1/2 d-theme-dark-bg\">\n              <div class=\"p-8\">\n                <div class=\"vx-card__title mb-8\">\n                  <h4 class=\"mb-4\">{{$ml.get('sign_in')}}</h4>\n                  <!--                                    <p>Welcome back, please login to your account.</p>-->\n                </div>\n                <vs-input\n                  name=\"email\"\n                  icon=\"icon icon-user\"\n                  icon-pack=\"feather\"\n                  :label-placeholder=\"$ml.get('username')\"\n                  v-model=\"username\"\n                  class=\"w-full no-icon-border\"/>\n                <span class=\"span-text-validation text-danger text-bold\" id=\"username_error\"></span>\n                <br>\n\n                <vs-input\n                  type=\"password\"\n                  name=\"password\"\n                  icon=\"icon icon-lock\"\n                  icon-pack=\"feather\"\n                  :label-placeholder=\"$ml.get('password')\"\n                  v-model=\"password\"\n                  class=\"w-full mt-6 no-icon-border\"/>\n                <span class=\"span-text-validation text-danger text-bold\" id=\"password_error\"></span>\n                <br>\n                <vs-button class=\"float-left\" @click=\"login()\">{{$ml.get('login')}}</vs-button>\n                <br>\n              </div>\n            </div>\n          </div>\n        </div>\n      </vx-card>\n    </div>\n  </div>\n</template>\n\n<script>\n\n  export default {\n    data() {\n      return {\n        username: '',\n        password: '',\n        checkbox_remember_me: false\n      }\n    },\n    methods: {\n      login() {\n        // AUTH_LOGIN\n        let vm = this;\n        $('.span-text-validation').text('')\n        try {\n          window.serviceAPI.API().post(window.serviceAPI.AUTH_LOGIN, {\n            username: vm.username,\n            password: vm.password,\n          })\n            .then((response) => {\n              response = response.data;\n              if (response.status) {\n                window.helper.showMessage('success', vm);\n                let auth_data = {\n                  admin: response.data.admin,\n                  token: response.data.token\n                };\n                window.ls.saveToStorage('auth_data',auth_data)\n                vm.$router.push({name: 'home'});\n                return null;\n              }\n            }).catch((error) => {\n            window.helper.handleError(error, vm);\n          });\n        } catch (e) {\n          console.log(e)\n        }\n      }\n    }\n  }\n</script>\n\n<style lang=\"scss\">\n  #page-login {\n    .social-login {\n      .bg-facebook {\n        background-color: #1551b1;\n      }\n\n      .bg-twitter {\n        background-color: #00aaff;\n      }\n\n      .bg-google {\n        background-color: #4285F4;\n      }\n\n      .bg-github {\n        background-color: #333;\n      }\n    }\n  }\n</style>\n"],"sourceRoot":"src/views/pages"}]}